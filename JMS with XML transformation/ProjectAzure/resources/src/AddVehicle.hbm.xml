<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd"> 

<hibernate-mapping>
   <class name="com.projectazure.hibernate.beans.VehicleBean.java" table="VEHICLE">
      
      <id name="vehicleId" column="VEHICLEID" type="int">
      <generator class="increment"></generator></id>
      <property name="vehicleType" column="VEHICLETYPE" type="java.lang.String"/>
      <property name="vehicleNO" column="VEHICLENO" type="java.lang.String"/>
      <property name="committedDate" column="COMMITTEDDATE" type="java.sql.Date"/>
       
       
   </class>
</hibernate-mapping>