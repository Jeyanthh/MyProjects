<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
 
<hibernate-mapping package="com.hiberonetomanymapping.hibernate">
 
    <class name="TourPackageBookingAllocation" table="ONE_TO_MANY_DEPARTMENT">
 
        <id name="bookingId" type="java.lang.Long" column="BOOKINGID" >
            <generator class="sequence" />
        </id>
         
        <property name="dateofTravel" column="DATEOFTRAVEL"  type = "date"/>
         <property name="dateofBooking" column="DATEOFBOOKING" type = "date"/>
          <property name="noofPassengers" column="NOOFPASSENGERS" type = "int" />
           <property name="requirements" column="REQUIREMENTS" type = "java.lang.String"/>
            <property name="availDiscount" column="AVAILDISCOUNT" type = ""/>
            <property name="status" column="STATUS" type= "java.lang.String"/>
            <property name="remark" column="REMARK" type = "java.lang.String"/>
            
 
        
    </class>
</hibernate-mapping>